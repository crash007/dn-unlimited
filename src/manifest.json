{
	"manifest_version": 2,
	"name": "DN Free",
	"version": "0.1",
	"description": "Unlimited number of free articles on dn.se. Removes all ads.",
	"homepage_url": "https://github.com/crash007/dn-free",
	"author": "crash007",
	 "content_scripts": [
		{
		"matches": ["http://www.dn.se/*","https://www.dn.se/*"],
		"js":["libs/jquery-3.2.1.min.js", "fixer.js"],
		 "run_at": "document_end"

		}
	],

	"icons":
		{
	          "128": "icon128.png"
		}

}
